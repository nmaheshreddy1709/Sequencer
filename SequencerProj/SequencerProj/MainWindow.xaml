<Window x:Class="SequencerProj.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SequencerProj"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Icon="/Sequence.png">
    <Grid x:Name="ProcessSequencer">
        <TabControl>
            <TabItem x:Name="ProcessExecuter" Header="Process Executer">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem x:Name="Admin" Header="Admin">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
