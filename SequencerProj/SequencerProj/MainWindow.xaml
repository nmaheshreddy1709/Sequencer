<Window x:Class="SequencerProj.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SequencerProj.View.InterfaceControls"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1200" Icon="/Sequence.png">
    <Grid x:Name="ProcessSequencer">
        <TabControl>
            <TabItem Name="tabProcessSeq" Header="Process Sequencer"/>
            <TabItem Name="tabProcessedit" Header="Process Editor"/>
        </TabControl>
        <Grid>
            <local:ProcessEditor></local:ProcessEditor>
        </Grid>
    </Grid>
</Window>
